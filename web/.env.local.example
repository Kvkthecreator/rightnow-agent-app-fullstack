# Local environment configuration
# Copy this to .env.local and adjust as needed

# Disable mock WebSocket features
NEXT_PUBLIC_DISABLE_MOCK_WEBSOCKET=true
NEXT_PUBLIC_USE_REAL_WEBSOCKET=true

# Development settings
NEXT_PUBLIC_DEBUG_REALTIME=true

# Override with local development URLs if needed
# NEXT_PUBLIC_API_BASE_URL=http://localhost:10000
# NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:3001