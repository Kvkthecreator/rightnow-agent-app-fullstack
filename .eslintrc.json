{
  "root": true,
  "env": {
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "paths": [{ "name": "@/lib/processors", "message": "Ingestion lives on the backend." }],
        "patterns": ["**/lib/processors/*"]
      }
    ]
  }
}
