{
  "scripts": {
    "agent:test": "tsx scripts/agent-test.ts",
    "agent:test:canon": "npm run agent:test -- --subset=canon",
    "agent:test:features": "npm run agent:test -- --subset=features",
    "agent:test:unit": "npm run agent:test -- --subset=unit",
    "agent:test:contracts": "npm run agent:test -- --subset=contracts",
    "test:canon": "playwright test --project=canon-v1.4 --reporter=list",
    "test:features": "playwright test --project=features --reporter=list",
    "test:e2e": "playwright test --project=e2e --reporter=list",
    "test:contracts": "playwright test --project=api-contracts --reporter=list",
    "test:unit": "npm --prefix web run test:unit",
    "test:integration": "playwright test --project=integration --reporter=list",
    "test:all": "npm run test:canon && npm run test:contracts && npm run test:features && npm run test:integration && npm run test:e2e",
    "seed:e2e": "tsx scripts/seed-e2e.ts",
    "test:debug": "playwright test --debug",
    "lint": "echo \"Skipping lint\"",
    "test": "npm run test:all",
    "codegen:supabase": "python scripts/gen_pydantic.py",
    "build:check": "npm --prefix web run build",
    "dev": "npm --prefix web run dev",
    "start": "npm --prefix web run start",
    "postinstall": "npm --prefix web install",
    "prepare": "husky install",
    "contracts:trace": "node scripts/contracts-trace.js",
    "contracts:check": "node -e \"process.exit(require('child_process').spawnSync('git', ['diff', '--quiet', '--', 'shared/contracts/REGISTRY.md']).status)\"",
    "guard:pipelines": "node scripts/check-pipeline-rpc-usage.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^22.15.30",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.1.5",
    "minimatch": "^9.0.5",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.7.1",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "glob": "^11.0.3",
    "react-hot-toast": "^2.5.2",
    "uuid": "^12.0.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.6"
  }
}
