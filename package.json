{
  "scripts": {
    "e2e:test": "playwright test",
    "e2e:debug": "playwright test --debug",
    "lint": "echo \"Skipping lint\"",
    "test": "echo \"No tests\"",
    "codegen:supabase": "python scripts/gen_pydantic.py",
    "build:check": "npm --prefix web run build",
    "postinstall": "npm --prefix web install",
    "prepare": "husky install",
    "contracts:trace": "node scripts/contracts-trace.js",
    "contracts:check": "node -e \"process.exit(require('child_process').spawnSync('git', ['diff', '--quiet', '--', 'shared/contracts/REGISTRY.md']).status)\"",
    "guard:pipelines": "node scripts/check-pipeline-rpc-usage.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^22.15.30",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^16.1.5",
    "minimatch": "^9.0.5",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.7.1",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "react-hot-toast": "^2.5.2",
    "zod": "^3.24.4",
    "zustand": "^5.0.6"
  },
  "lint-staged": {
    "web/**/*.{ts,tsx}": [
      "npx --yes --prefix web eslint --fix",
      "npx --yes --prefix web prettier --write"
    ]
  }
}
